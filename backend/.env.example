# Kalshi Live Environment
KALSHI_LIVE_API_KEY=your-live-api-key-uuid
KALSHI_LIVE_PRIVATE_KEY_PATH=./keys/live_private_key.pem

# Kalshi Demo Environment
KALSHI_DEMO_API_KEY=your-demo-api-key-uuid
KALSHI_DEMO_PRIVATE_KEY_PATH=./keys/demo_private_key.pem

# PostgreSQL (Supabase)
DATABASE_URL=postgresql://user:password@host:5432/dbname

# Server
CONTROL_API_HOST=127.0.0.1
CONTROL_API_PORT=8100

# Active environment (live or demo)
ACTIVE_ENVIRONMENT=demo

# Trading
GLOBAL_TRADING_ENABLED=false
